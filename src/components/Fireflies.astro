    .firefly {
        position: fixed;
        left: 50%;
        top: 50%;
        width: 0.4vw;
        height: 0.4vw;
        margin: -0.2vw 0 0 9.8vw;
        animation: ease 200s alternate infinite;
        pointer-events: none;
    }

    .firefly::before,
    .firefly::after {
        content: "";
        position: absolute;
        width: 100%;
        height: 100%;
        border-radius: 50%;
        transform-origin: -10vw;
    }
    .firefly::before {
        background: #ff2020;
        opacity: 0.8;
        animation: drift ease alternate infinite;
    }
    .firefly::after {
        background: #ffa500;
        opacity: 0;
        box-shadow: 0 0 0vw 0vw #ffa500;
        animation:
            drift ease alternate infinite,
            flash ease infinite;
    }

